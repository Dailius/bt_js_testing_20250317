  "type": "commonjs",
  "devDependencies": {
    "concurrently": "^9.2.0",
    "cypress": "^14.5.4",
    "http-server": "^14.1.1",
    "open": "^10.2.0",
    "wait-on": "^8.0.4"
  }
}
